#include <stdio.h>
int main(void) {
	int t,a,b,c,d,k;
	scanf("%d", &t);
	while(t--)
	{
	    scanf("%d%d%d%d%d", &a, &b, &c, &d, &k);
	    int x = abs(c-a)+abs(b-d);
	    if(x>k){
	        printf("NO\n");
	    }
	    else if(x==k){
	        printf("Yes\n");
	    }
	    else if((k-x)%2==0){
	        printf("Yes\n");
	    }
	    else{
	        printf("No\n");
	    }
	}
	return 0;
}

