#include <stdio.h>
int main(void) {
	int t, n, k;
	int h, count;
	scanf("%d",&t);
	while(t--)
	{
	    for(int i = 0; i < n; i++)
	    {
	        scanf("%d%d", &n, &k);
	        for(int j = 0; j < n; j++)
	        {
	            scanf("%d",&h);
	            if(h > k)
	                {
	                    count += 1;
	                }
	        }
	        printf("%d\n",count);
            count=0;
	    }
	    
	}
	return 0;
}

