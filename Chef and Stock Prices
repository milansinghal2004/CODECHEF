#include <stdio.h>

int main(void) {
	int t,s,a,b,c;
	scanf("%d",&t);
	while(t--)
	{
	    scanf("%d%d%d%d",&s ,&a ,&b ,&c);
	    float np = s+((c/100)*s);
	    if(np>a && np<b)
	    {
	        printf("Yes\n");
	    }
	    else
	    {
	        printf("No\n");
	    }
	}
	return 0;
}

